	<parameter name="PIATRI_ANNTRI" class="java.math.BigDecimal"/>
	<parameter name="PIATRI_NOTSCHE3" class="java.lang.String"/>
	<parameter name="UFFINT_NOMEIN" class="java.lang.String"/>
